{
  "categories": [
    {
      "title": "## Features",
      "labels": ["Status: ready for review"]
    },
    {
      "title": "## Other changes",
      "labels": []
    }
  ],
  "ignore_labels": [],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
  "pr_template": "* [#${{NUMBER}}](${{URL}}) ${{TITLE}} ([${{AUTHOR}}](${{AUTHOR}}))",

  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "flags": "gu"
    },
    {
      "pattern": "\\[Issue\\]",
      "on_property": "title",
      "method": "match"
    }
  ],
  "duplicate_filter": {
    "pattern": "\\[ABC-....\\]",
    "on_property": "title",
    "method": "match"
  },
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
      "target": "- $4\n  - $6"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [
    "Owner/qa"
  ],
  "tag_resolver": {
    "method": "semver",
    "filter": {
      "pattern": "api-(.+)",
      "flags": "gu"
    }
  },
  "base_branches": ["main", "master", "develop", "staging"]
}